<div class="popup-window logout" id="popup-1">
    <div class="overlay"></div>
    <div class="content">
        <h2>Are you sure you want to Log out?</h2>
        <div class="img">
            <img class="profile" src="resources/warn.png">
            
        </div>
        <div class="close-btn" onclick="toggleLogout()">&times;</div>
        <div class="form-control">
            <button class="submit-logout submit button logout" onclick="submitLogout()">
                
                <span class="buttonText">Confirm</span>
            </button>
        </div>
    </div>
</div>